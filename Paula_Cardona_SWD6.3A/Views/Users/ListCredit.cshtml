@model IEnumerable<Common.UserData>

@{
    ViewData["Title"] = "ListCredit";
}

<h1>List Credits - User</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfCredits)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfCredits)
            </td>

        </tr>
}
    </tbody>
</table>
<div>
    <a asp-action="List">Click here to go back to your list of items!</a>
</div>